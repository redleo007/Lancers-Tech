# MongoDB Atlas Connection (Primary)
MONGODB_USERNAME=your_atlas_username
MONGODB_PASSWORD=your_atlas_password
MONGODB_CLUSTER=cluster0.xxxxx.mongodb.net
MONGODB_DATABASE=sprintzen

# Environment
NODE_ENV=development
PORT=5000
HOST=localhost

# Security
JWT_SECRET=replace_with_secure_secret
JWT_AUDIENCE=sprintzen-app
JWT_ISSUER=sprintzen-auth
SESSION_SECRET=replace_with_secure_secret
DOMAIN=localhost

# Rate Limiting
RATE_LIMIT_WINDOW_MS=900000
RATE_LIMIT_MAX=100

# OAuth
GOOGLE_CLIENT_ID=your-google-client-id
GOOGLE_CLIENT_SECRET=your-google-client-secret

# CORS
CORS_ORIGIN=http://localhost:5173